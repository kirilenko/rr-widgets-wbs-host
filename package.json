{
  "name": "rr-widgets-wbs-host",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "18",
    "pnpm": "7"
  },
  "type": "module",
  "scripts": {
    "setup": "scripts/nvm-use.sh && pnpm i",
    "dev": "vite --port 3001 --strictPort",
    "lint:styles": "npx stylelint --fix 'src/**/**.{css,style.ts,style.tsx}'",
    "lint:scripts": "eslint --fix 'src/**/**.{ts,tsx}' './.*.cjs' './*.{cjs,ts}'",
    "format": "pnpm lint:styles && pnpm lint:scripts && prettier --write ./*.{json,md} && scripts/postformat.sh",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview --port 3001 --strictPort"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.2.1",
    "@types/node": "18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "0.3.1",
    "prettier": "^2.6.2",
    "stylelint": "^15.2.0",
    "stylelint-config-css-modules": "4.2.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-config-recommended-scss": "9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.2",
    "stylelint-order-config-standard": "^0.1.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}
